<odoo>
    <record id="ir_mail_server_form" model="ir.ui.view">
        <field name="name">ir_mail_server_form</field>
        <field name="model">ir.mail_server</field>
        <field name="inherit_id" ref="base.ir_mail_server_form"/>
        <field name="priority">75</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='smtp_host']" position="before">
                <field name="from_method"/>
            </xpath>
        </field>
    </record>
</odoo>
